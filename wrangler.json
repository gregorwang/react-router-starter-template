{
	"$schema": "./node_modules/wrangler/config-schema.json",
	"name": "react-router-starter-template",
	"main": "./workers/app.ts",
	"compatibility_date": "2025-10-08",
	"compatibility_flags": [
		"nodejs_compat"
	],
	"observability": {
		"enabled": true
	},
	"upload_source_maps": true,
	"ai": {
		"binding": "AI"
	},
	"vars": {
		"VALUE_FROM_CLOUDFLARE": "Hello from Cloudflare"
	},
	"ratelimits": [
		{
			"name": "CHAT_RATE_LIMITER",
			"namespace_id": "1001",
			"simple": {
				"limit": 20,
				"period": 60
			}
		}
	],
	"d1_databases": [
		{
			"binding": "DB",
			"database_name": "d1database",
			"database_id": "d71d0cff-0de4-406f-b1a2-c74e095878e7",
			"migrations_dir": "app/lib/db/migrations"
		}
	],
	"kv_namespaces": [
		{
			"binding": "SETTINGS_KV",
			"id": "37122faa1e904d958cd4b08e1aad83b2"
		}
	],
	"r2_buckets": [
		{
			"bucket_name": "llm-platform-archive-20260202",
			"binding": "CHAT_ARCHIVE"
		},
		{
			"bucket_name": "llm-platform-archive-20260202",
			"binding": "CHAT_MEDIA"
		}
	],
	"durable_objects": {
		"bindings": [
			{
				"name": "CHAT_RATE_LIMITER_DO",
				"class_name": "ChatRateLimiter"
			}
		]
	},
	"migrations": [
		{
			"tag": "v1",
			"new_sqlite_classes": [
				"ChatRateLimiter"
			]
		}
	]
}
